{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/get", "dest": "/api/get.js" },
    { "src": "/search", "dest": "/api/search.js" },
    { "src": "/view", "dest": "/api/view.js" },
    { "src": "/save/ip", "dest": "/api/save/[ip].js" },
    { "src": "/", "dest": "/index.html" }
  ]
}
